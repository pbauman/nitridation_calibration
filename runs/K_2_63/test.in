#
#

[GammaCNInverseProblem]

sigma = '2.0e-8' # [kg]

mass_loss = '2.45e-6' # [kg]

gamma_nominal_value = '4.5e-3' 

[]

[BoundaryConditions] 

[./TubeWall]

# Locaton of thermocouples along tube wall (m)
tc_locs = '0.0 0.244 0.324 0.639 0.709 0.869'

# Temperature vals for above locs (K) - at 1273K
wall_temps = '298.0 350.0 1273.0 1273.0 525.0 400.0'

[../InletProfile]

mdot = '16.5' # [mg/s]

p_inlet = '600' # [Pa]

[]

[QoI]

enabled_qois = 'MassLoss'

[./MassLoss]

bc_ids = '3'

radius = '0.0015875'
length = '0.029'
delta_t = '8160'

[]

# Options related to all Physics
[Physics]

enabled_physics = 'ReactingLowMachNavierStokes'

[./Chemistry]

species   = 'N2 N C CN'
chem_file = './nitridation.xml'
mixture   = 'air4sp'

[../Transport]

viscosity = '1.0e-5'
thermal_conductivity = '.04'
Lewis_number = '1.4'

# Options for Incompressible Navier-Stokes physics
[../ReactingLowMachNavierStokes]

species_FE_family = 'LAGRANGE'
V_FE_family       = 'LAGRANGE'
P_FE_family       = 'LAGRANGE'
T_FE_family       = 'LAGRANGE'

species_order = 'SECOND'
V_order       = 'SECOND'
T_order       = 'SECOND'
P_order       = 'FIRST'

# Thermodynamic pressure
p0 = '530.6' #[Pa]

# Gravity vector
g = '0.0 0.0' #[m/s^2]

chemistry_library      = 'grins'
transport_library      = 'grins_constant'
thermodynamics_library = 'grins_cea'

# Boundary ids:
# inflow = 1
# centerline (without sample) = 2
# specimen = 3
# outer wall = 4
# outflow = 5

vel_bc_ids = '1 2 3 4'
vel_bc_types = 'general_velocity axisymmetric no_slip no_slip'

general_velocity_fix_1 = 'u'

temp_bc_ids = '1 2 3 4'
temp_bc_types = 'isothermal axisymmetric isothermal general_isothermal'

T_wall_1 = '298'
T_wall_3 = '1273'

#species_bc_ids = '1 3'
#species_bc_types = 'prescribed_mole_fracs catalytic_wall'

species_bc_ids = '1 3 4' 
species_bc_types = 'prescribed_mole_fracs catalytic_wall catalytic_wall'

bound_species_1 = '0.596 0.404 0.0 0.0'

wall_catalytic_reactions_3 = 'N->CN'
gamma_N_3 = '${GammaCNInverseProblem/gamma_nominal_value}'

# This is currently a dummy.
gamma_CN_3 = '${Physics/ReactingLowMachNavierStokes/gamma_N_3}'

wall_catalytic_reactions_4 = 'N->N2'
gamma_N_4 = '1.0e-4'

# This is currently a dummy.
gamma_N2_4 = '${Physics/ReactingLowMachNavierStokes/gamma_N_4}'

enable_thermo_press_calc = 'false'
pin_pressure = 'false'

[]

[restart-options]

#restart_file = 'restart.xdr'

# Mesh related options
[mesh-options]
mesh_option = 'read_mesh_from_file'
mesh_filename = 'K_2_63_axi.exo'

[]

# Options for tiem solvers
[unsteady-solver]
transient = 'false' 

[]

#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations = 30
max_linear_iterations = 2500

verify_analytic_jacobians = 0.0

initial_linear_tolerance = 1.0e-10

use_numerical_jacobians_only = 'true'

[]

# Visualization options
[vis-options]
output_vis = 'true'

vis_output_file_prefix = 'nitridation' 

output_residual = 'false'

output_format = 'ExodusII xdr'

#output_vars = 'cp_mix h_s rho_mix mole_fractions omega_dot'
output_vars = 'mole_fractions'

# Options for print info to the screen
[screen-options]

system_name = 'Nitridation'

print_equation_system_info = 'true'
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

print_element_jacobians = 'false'

print_qoi = 'true'
echo_qoi = 'true'

[]

[../VariableNames]

Temperature = 'T'
u_velocity = 'u'
v_velocity = 'v'
w_velocity = 'w'
pressure = 'p'

[]
